<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>fournisseurToutbois</display-name>
  <servlet>
    <servlet-name>Accueil</servlet-name>
    <servlet-class>com.mika.servlets.Accueil</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Accueil</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>NouveauFournisseur</servlet-name>
    <servlet-class>com.mika.servlets.NouveauFournisseur</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>NouveauFournisseur</servlet-name>
    <url-pattern>/NouveauFournisseur.java</url-pattern>
  </servlet-mapping>
  <jsp-config>
    <jsp-property-group>
      <url-pattern>*.jsp</url-pattern>
      <include-prelude>/WEB-INF/inc/taglib.jsp</include-prelude>
    </jsp-property-group>
  </jsp-config>
</web-app>